{
  "name": "user",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "user_id": {
      "type": "string"
    },
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    },
    "home_address": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "owner": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasOne",
      "model": "AccessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    },
    "orders": {
      "type": "hasMany",
      "model": "order",
      "foreignKey": "userId"
    },
    "restaurants": {
      "type": "hasMany",
      "model": "restaurant",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
